{
  "expo": {
    "name": "MemoTile",
    "slug": "memotile-app-anti",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/998b0866-55ab-44aa-a1b1-9b82af8c3fe1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "memotile",
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "@react-native-firebase/app",
        {
          "ios": {
            "use_modular_headers": true
          }
        }
      ],
      "@react-native-firebase/auth"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.benjiwon.memotileappanti",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:memotile.com",
        "applinks:www.memotile.com"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "memotile",
              "com.googleusercontent.apps.166543035480-d0hra4ce54ljrvnka86v6n0tvfkrt25b"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.benjiwon.memotileappanti",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "memotile.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.memotile.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "998b0866-55ab-44aa-a1b1-9b82af8c3fe1"
      },
      "googleExpoClientId": "166543035480-m69rkgq6uhqi56ntbbgdbst66cc23a50.apps.googleusercontent.com",
      "googleIosClientId": "166543035480-d0hra4ce54ljrvnka86v6n0tvfkrt25b.apps.googleusercontent.com",
      "googleAndroidClientId": "166543035480-i5m8vphpi7bav4iu5e9s1vkad08sfsle.apps.googleusercontent.com",
      "googleWebClientId": "166543035480-m69rkgq6uhqi56ntbbgdbst66cc23a50.apps.googleusercontent.com"
    }
  }
}